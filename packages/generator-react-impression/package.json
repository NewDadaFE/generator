{
  "name": "generator-react-impression",
  "version": "2.1.5",
  "keywords": ["yeoman-generator"],
  "description": "Yeoman generator for React project.",
  "repository": "NewDadaFE/generator",
  "license": "MIT",
  "engines": {
    "node": ">=8.12.0",
    "yarn": ">=1.9.4"
  },
  "files": ["generators"],
  "scripts": {
    "precommit": "lint-staged",
    "format": "prettier-eslint --write '**/*.{js,json}'"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "mkdirp": "~0.5.1",
    "ramda": "~0.24.1",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.1"
  },
  "devDependencies": {
    "eslint-config-react-impression": "~2.0.0",
    "husky": "~0.14.3",
    "lint-staged": "~4.1.3",
    "prettier-eslint-cli": "~4.3.2"
  },
  "eslintConfig": {
    "extends": "react-impression"
  },
  "lint-staged": {
    "*.{js,json}": ["prettier-eslint --write", "git add"]
  }
}
